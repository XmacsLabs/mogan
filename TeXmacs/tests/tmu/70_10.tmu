<TMU|<tuple|1.0.2|1.2.8-rc3>>

<style|<tuple|generic|no-page-numbers|chinese|goldfish>>

<\body>
  <\goldfish-code>
    (+ 1 2)

    (\<gtr\> 1 2)

    (\<less\> 1 2)

    (* 1 2)

    (/ 1 2)

    (= 1 2)

    (define x-define 1)

    (define ?define 1)

    'read-error

    \;

    define-x

    x-define

    \;

    definex

    xdefine
  </goldfish-code>

  <\session|scheme|default>
    <\unfolded-io|Scheme] >
      (parser-feature "goldfish" "identifier")
    <|unfolded-io>
      (identifier (extra_chars "?" "+" "-" "." "!" "*" "\<gtr\>" "=" "\<less\>") (start_chars "?" "+" "-" "." "!" "*" "\<gtr\>" "=" "\<less\>"))
    </unfolded-io>

    <\input|Scheme] >
      \;
    </input>
  </session>

  <\session|goldfish|default>
    <\output>
      Goldfish Scheme 17.10.0 Community Edition by LiiiLabs

      implemented on S7 Scheme (10.11, 2-July-2024)
    </output>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      (define ?and 1)
    <|unfolded-io>
      <goldfish-result|1>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      ?and
    <|unfolded-io>
      <goldfish-result|1>
    </unfolded-io>
  </session>
</body>

<\initial>
  <\collection>
    <associate|page-medium|paper>
    <associate|page-screen-margin|false>
  </collection>
</initial>