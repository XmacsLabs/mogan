<TeXmacs|2.1.2>

<style|<tuple|generic|no-page-numbers|british>>

<\body>
  Helllo

  <\session|scheme|default>
    <\unfolded-io|Scheme] >
      (use-modules (binary hunspell))
    <|unfolded-io>
      (#1=(inlet 'supports-slidemove? supports-slidemove?
      'scala-snippet-\<gtr\>texmacs scala-snippet-\<gtr\>texmacs
      'scala-\<gtr\>texmacs scala-\<gtr\>texmacs 'texmacs-\<gtr\>scala
      texmacs-\<gtr\>scala 'python-snippet-\<gtr\>texmacs
      python-snippet-\<gtr\>texmacs 'python-\<gtr\>texmacs
      python-\<gtr\>texmacs 'texmacs-\<gtr\>python texmacs-\<gtr\>python
      'julia-snippet-\<gtr\>texmacs julia-snippet-\<gtr\>texmacs
      'julia-\<gtr\>texmacs julia-\<gtr\>texmacs 'texmacs-\<gtr\>julia
      texmacs-\<gtr\>julia 'javascript-snippet-\<gtr\>texmacs
      javascript-snippet-\<gtr\>texmacs 'javascript-\<gtr\>texmacs
      javascript-\<gtr\>texmacs ...))
    </unfolded-io>

    <\unfolded-io|Scheme] >
      (find-binary-hunspell)
    <|unfolded-io>
      \<less\>url /usr/bin/hunspell\<gtr\>
    </unfolded-io>

    <\unfolded-io|Scheme] >
      (has-binary-hunspell?)
    <|unfolded-io>
      #t
    </unfolded-io>

    <\input|Scheme] >
      \;
    </input>
  </session>

  <\session|scheme|default>
    <\unfolded-io|Scheme] >
      (use-modules (binary aspell))
    <|unfolded-io>
      (#1=(inlet 'supports-slidemove? supports-slidemove?
      'scala-snippet-\<gtr\>texmacs scala-snippet-\<gtr\>texmacs
      'scala-\<gtr\>texmacs scala-\<gtr\>texmacs 'texmacs-\<gtr\>scala
      texmacs-\<gtr\>scala 'python-snippet-\<gtr\>texmacs
      python-snippet-\<gtr\>texmacs 'python-\<gtr\>texmacs
      python-\<gtr\>texmacs 'texmacs-\<gtr\>python texmacs-\<gtr\>python
      'julia-snippet-\<gtr\>texmacs julia-snippet-\<gtr\>texmacs
      'julia-\<gtr\>texmacs julia-\<gtr\>texmacs 'texmacs-\<gtr\>julia
      texmacs-\<gtr\>julia 'javascript-snippet-\<gtr\>texmacs
      javascript-snippet-\<gtr\>texmacs 'javascript-\<gtr\>texmacs
      javascript-\<gtr\>texmacs ...))
    </unfolded-io>

    <\unfolded-io|Scheme] >
      (find-binary-aspell)
    <|unfolded-io>
      \<less\>url /usr/bin/aspell\<gtr\>
    </unfolded-io>

    <\unfolded-io|Scheme] >
      (has-binary-aspell?)
    <|unfolded-io>
      #t
    </unfolded-io>

    <\input|Scheme] >
      \;
    </input>
  </session>
</body>

<\initial>
  <\collection>
    <associate|page-medium|paper>
    <associate|page-screen-margin|false>
  </collection>
</initial>