<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>MoganResearch</Name>
    <Title>Mogan Research installer</Title>
    <Publisher>Xmacslabs</Publisher>
    <ProductUrl>https://mogan.app/</ProductUrl>
    <InstallerApplicationIcon>Xmacs</InstallerApplicationIcon>
    <InstallerWindowIcon>new-mogan-512.png</InstallerWindowIcon>
    <Logo>new-mogan-32.png</Logo>
    <StartMenuDir>MoganResearch</StartMenuDir>
    <Version>${XMACS_VERSION}</Version>
    <TargetDir>@ApplicationsDir@/XmacsLabs/MoganResearch-${XMACS_VERSION}</TargetDir>
    <SupportsModify>true</SupportsModify>
    <RunProgram>@TargetDir@/bin/MoganResearch.exe</RunProgram>
</Installer>